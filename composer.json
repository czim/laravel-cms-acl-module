{
    "name": "czim/laravel-cms-acl-module",
    "description": "Laravel CMS: ACL Module",
    "keywords": [ "cms", "laravel", "acl" ],
    "homepage": "https://github.com/czim",
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "czim/laravel-cms-core": "~1.5",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.5",
        "orchestra/database": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "Czim\\CmsAclModule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\CmsAclModule\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
